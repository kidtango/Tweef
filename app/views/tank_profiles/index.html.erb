<p id="notice"><%= notice %></p>

<h1>Tank Profiles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tank_profiles.each do |tank_profile| %>
      <tr>
        <td><%= link_to 'Show', tank_profile %></td>
        <td><%= link_to 'Edit', edit_tank_profile_path(tank_profile) %></td>
        <td><%= link_to 'Destroy', tank_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tank Profile', new_tank_profile_path %>
